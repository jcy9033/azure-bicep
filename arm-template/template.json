{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "vaults_chanpu_kv_1_name": {
            "defaultValue": "chanpu-kv-1",
            "type": "String"
        }
    },
    "variables": {},
    "resources": [
        {
            "type": "Microsoft.KeyVault/vaults",
            "apiVersion": "2023-07-01",
            "name": "[parameters('vaults_chanpu_kv_1_name')]",
            "location": "japaneast",
            "properties": {
                "sku": {
                    "family": "A",
                    "name": "Standard"
                },
                "tenantId": "e25d89fa-cf6f-41ea-ad14-451e723f2900",
                "networkAcls": {
                    "bypass": "AzureServices",
                    "defaultAction": "Deny",
                    "ipRules": [],
                    "virtualNetworkRules": []
                },
                "accessPolicies": [],
                "enabledForDeployment": false,
                "enabledForDiskEncryption": false,
                "enabledForTemplateDeployment": false,
                "enableSoftDelete": true,
                "softDeleteRetentionInDays": 90,
                "enableRbacAuthorization": false,
                "vaultUri": "[concat('https://', parameters('vaults_chanpu_kv_1_name'), '.vault.azure.net/')]",
                "provisioningState": "Succeeded",
                "publicNetworkAccess": "Disabled"
            }
        },
        {
            "type": "Microsoft.KeyVault/vaults/privateEndpointConnections",
            "apiVersion": "2023-07-01",
            "name": "[concat(parameters('vaults_chanpu_kv_1_name'), '/', parameters('vaults_chanpu_kv_1_name'), '-pep_10442308-5b02-4d51-8b69-669e8bcf8051')]",
            "location": "japaneast",
            "dependsOn": [
                "[resourceId('Microsoft.KeyVault/vaults', parameters('vaults_chanpu_kv_1_name'))]"
            ],
            "properties": {
                "provisioningState": "Succeeded",
                "privateEndpoint": {},
                "privateLinkServiceConnectionState": {
                    "status": "Approved",
                    "actionsRequired": "None"
                }
            }
        }
    ]
}